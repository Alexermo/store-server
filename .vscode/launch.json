    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "justMyCode": true
        },
        {
            "name": "store",
            "python": "C:/Учёба/store-server/venv/scripts/python",
            "type": "python",
            "request": "launch",
            "program": "C:/Учёба/store-server/store/manage.py",
            "console": "integratedTerminal",
            "args": [
                "runserver"
            ]
        },
        {
            "name": "Django",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "program": "C:/Учёба/store-server/store/manage.py",
            "args": [
                "runserver",
                "--no-color",
                "--noreload"
            ],
            "debugOptions": [
                "WaitOnAbnormalExit",
                "WaitOnNormalExit",
                "RedirectOutput",
                "DjangoDebugging"
            ]
        },
        {
            "name": "Python: Django",
            "type": "python",
            "python": "C:/Учёба/store-server/venv/scripts/python",
            "request": "launch",
            "program": "C:/Учёба/store-server/store/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "justMyCode": false
        }
    ]
}